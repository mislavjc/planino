{
  "name": "planino",
  "version": "0.1.9",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "storybook": "turbo storybook",
    "lint": "turbo lint",
    "knip": "knip",
    "commonality:studio": "commonality studio",
    "deploy:importer": "turbo run deploy --filter=importer",
    "prepare": "husky",
    "db:generate": "turbo db:generate --filter=database",
    "db:migrate": "turbo db:migrate --filter=database",
    "db:studio": "turbo db:studio --filter=database",
    "generate:importer-client": "turbo generate:importer-client --filter=dashboard",
    "chagelog:patch": "changelogen --patch --release",
    "changelog:minor": "changelogen --minor --release",
    "changelog:major": "changelogen --major --release"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "changelogen": "^0.5.5",
    "husky": "^9.0.11",
    "knip": "^5.15.1",
    "prettier": "^3.2.5",
    "turbo": "^2.0.3"
  },
  "volta": {
    "node": "20.12.2"
  },
  "dependencies": {
    "commonality": "^0.2.63",
    "commonality-checks-recommended": "^0.0.40"
  }
}
